---
import videojs from 'video.js';
import 'video.js/dist/video-js.css';
import LucidVideo from '../assets/videos/lucid.mp4'
---

<style>
 
</style>

<div>
    <video
    id="my-video"
    class="video-js"
    controls
    preload="auto"
    width="640"
    height="400"
    data-setup="{}"
  >
    <source src={LucidVideo} type="video/mp4" />
    <p class="vjs-no-js">
      To view this video please enable JavaScript, and consider upgrading to a
      web browser that
      <a href="https://videojs.com/html5-video-support/" target="_blank"
        >supports HTML5 video</a
      >
    </p>
  </video>
</div>